<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/background"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_other"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.tception.LevelOneSingle" >

    <ImageButton
        android:id="@+id/backButtonClassic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:background="@drawable/clickedback"
        android:src="@drawable/clickedback" />

    <ImageButton
        android:id="@+id/cell_1_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/cell_2_2"
        android:layout_toLeftOf="@+id/cell_1_3"
        android:paddingBottom="0dp"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"
        android:paddingTop="0dp"
        android:scaleType="fitXY"
        android:src="@drawable/one"
         android:background="@android:color/transparent" />

    <ImageButton
        android:id="@+id/cell_1_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/cell_1_2"
        android:layout_toLeftOf="@+id/cell_1_2"
        android:paddingBottom="0dp"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"
        android:paddingTop="0dp"
        android:scaleType="fitXY"
        android:src="@drawable/one"
         android:background="@android:color/transparent" />

    <ImageButton
        android:id="@+id/cell_3_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/cell_2_2"
        android:layout_toLeftOf="@+id/cell_1_3"
        android:paddingBottom="0dp"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"
        android:paddingTop="0dp"
        android:scaleType="fitXY"
        android:src="@drawable/one"
         android:background="@android:color/transparent" />

    <ImageButton
        android:id="@+id/cell_2_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/cell_3_2"
        android:layout_toRightOf="@+id/cell_2_2"
        android:paddingBottom="0dp"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"
        android:paddingTop="0dp"
        android:scaleType="fitXY"
        android:src="@drawable/one"
         android:background="@android:color/transparent" />

    <ImageButton
        android:id="@+id/cell_2_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:paddingBottom="0dp"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"
        android:paddingTop="0dp"
        android:scaleType="fitXY"
        android:src="@drawable/one"
         android:background="@android:color/transparent" />

    <ImageButton
        android:id="@+id/cell_2_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/cell_3_2"
        android:layout_toLeftOf="@+id/cell_3_2"
        android:paddingBottom="0dp"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"
        android:paddingTop="0dp"
        android:scaleType="fitXY"
        android:src="@drawable/one"
         android:background="@android:color/transparent" />

    <ImageButton
        android:id="@+id/cell_3_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/cell_2_3"
        android:layout_alignTop="@+id/cell_3_2"
        android:paddingBottom="0dp"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"
        android:paddingTop="0dp"
        android:scaleType="fitXY"
        android:src="@drawable/one"
         android:background="@android:color/transparent" />

    <ImageButton
        android:id="@+id/cell_3_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/cell_2_1"
        android:layout_below="@+id/cell_2_1"
        android:paddingBottom="0dp"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"
        android:paddingTop="0dp"
        android:scaleType="fitXY"
        android:src="@drawable/one"
         android:background="@android:color/transparent" />

    <ImageButton
        android:id="@+id/cell_1_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/cell_2_3"
        android:layout_alignLeft="@+id/cell_2_3"
        android:paddingBottom="0dp"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"
        android:paddingTop="0dp"
        android:scaleType="fitXY"
        android:src="@drawable/one"
         android:background="@android:color/transparent" />

    <ImageButton
        android:id="@+id/xwins"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:src="@drawable/xwins"
        android:visibility="gone" 
        android:background="@android:color/transparent"/>
    
     <ImageButton
         android:id="@+id/owins"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_centerHorizontal="true"
         android:layout_centerVertical="true"
         android:src="@drawable/owins"
         android:visibility="gone" 
         android:background="@android:color/transparent"/>
     
      <ImageButton
          android:id="@+id/tie"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_centerHorizontal="true"
          android:layout_centerVertical="true"
          android:src="@drawable/draw"
          android:visibility="gone" 
          android:background="@android:color/transparent"/>

      <ImageView
         android:id="@+id/imageView1"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignParentTop="true"
         android:layout_centerHorizontal="true"
         android:src="@drawable/classictitle" />
</RelativeLayout>
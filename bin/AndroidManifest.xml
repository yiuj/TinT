<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tintpackage"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_final"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.tception.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
    		android:configChanges="keyboardHidden|orientation|screenSize" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name = "com.example.tception.Settings"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
    		android:configChanges="keyboardHidden|orientation|screenSize">
         </activity>
         <activity
            android:name = "com.example.tception.SinglePlayer"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
    		android:configChanges="keyboardHidden|orientation|screenSize">
         </activity>
         <activity
            android:name = "com.example.tception.Multiplayer"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
    		android:configChanges="keyboardHidden|orientation|screenSize">
         </activity>
          <activity
            android:name = "com.example.tception.LevelOneSingle"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
    		android:configChanges="keyboardHidden|orientation|screenSize">
         </activity>
          <activity
            android:name = "com.example.tception.LevelSixSingle"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
    		android:configChanges="keyboardHidden|orientation|screenSize">
         </activity>
         <activity
            android:name = "com.example.tception.LevelOneMulti"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
    		android:configChanges="keyboardHidden|orientation|screenSize">
         </activity>
          <activity
            android:name = "com.example.tception.LevelSixMulti"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
    		android:configChanges="keyboardHidden|orientation|screenSize">
         </activity>
         <activity
            android:name = "com.example.tception.XtremeInstructions"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
    		android:configChanges="keyboardHidden|orientation|screenSize">
         </activity>
    </application>

</manifest>
